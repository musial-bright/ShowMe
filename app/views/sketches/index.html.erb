<h1>Listing sketches</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sketches.each do |sketch| %>
  <tr>
    <td><%= sketch.name %></td>
    <td><%= sketch.data %></td>
    <td><%= link_to 'Show', sketch %></td>
    <td><%= link_to 'Edit', edit_sketch_path(sketch) %></td>
    <td><%= link_to 'Destroy', sketch, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sketch', new_sketch_path %>
